{
  "name": "augustana-health-admin-menu",
  "description": "",
  "version": "0.1.0",
  "private": true,
  "author": "",
  "copyright": "",
  "homepage": "",
  "main": "temp/background.js",
  "build": {
    "appId": "com.example.augustana-health-admin-menu",
    "files": [
      "temp/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha --timeout=10000 temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder",
    "lint": "lint-staged"
  },
  "pre-commit": [
    "lint"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.css": [
      "stylelint --syntax=css",
      "git add"
    ],
    "*.pug": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "electron-pug": "^2.0.0",
    "electron-settings": "^3.2.0",
    "fs-jetpack": "^2.1.0",
    "googleapis": "^39.2.0",
    "sequelize": "^5.21.2",
    "sqlite3": "^4.1.0",
    "string": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@prettier/plugin-pug": "^1.1.0",
    "babel-loader": "^8.0.0-beta.4",
    "chai": "^4.1.0",
    "codecov": "^3.6.4",
    "css-loader": "^3.1.0",
    "electron": "^4.0.0",
    "electron-builder": "^21.1.5",
    "electron-mocha": "^8.1.1",
    "eslint": "^6.7.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.0",
    "nyc": "^15.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "serialize-javascript": ">=2.1.1",
    "source-map-support": "^0.5.6",
    "spectron": "^6.0.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.1.3",
    "tailwindcss-grid": "^1.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
